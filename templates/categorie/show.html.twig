{% extends 'base.html.twig' %}

{% block title %}Categorie{% endblock %}

{% block body %}
    <h1>Categorie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ categorie.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ categorie.libelle }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ categorie.statut ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ categorie.slug }}</td>
            </tr>
            <tr>
                <th>PubliePar</th>
                <td>{{ categorie.publiePar }}</td>
            </tr>
            <tr>
                <th>PublieLe</th>
                <td>{{ categorie.publieLe }}</td>
            </tr>
            <tr>
                <th>ModifiePar</th>
                <td>{{ categorie.modifiePar }}</td>
            </tr>
            <tr>
                <th>ModifieLe</th>
                <td>{{ categorie.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('categorie_index') }}">back to list</a>

    <a href="{{ path('categorie_edit', {'id': categorie.id}) }}">edit</a>

    {{ include('categorie/_delete_form.html.twig') }}
{% endblock %}